{
  "name": "Notebook",
  "locals": {
    "page_size": 100,
    "lang": "en",
    "sort_by": "id",
    "background_color": "#1e1e1e",
    "text_color": "rgb(220, 221, 222)",
    "link_color": "rgb(68 144 225)",
    "syntax_highlighter": {
      "id": "solarized-light"
    },
    "syntax_highlighter_font": {
      "id": "plex-mono"
    },
    "body_font": {
      "id": "system-sans"
    },
    "demo_folder": "notebook"
  },
  "views": {
    "pagination.html": {
      "url": "/pagination/:page"
    },
    "entries.html": {
      "partials": {
        "title": "{{#entries}}{{#first}}{{{title}}} -  {{/first}}{{/entries}}{{{title}}}",
        "description": "{{#entries}}{{#first}}{{{title}}} -  {{/first}}{{/entries}}{{{title}}}"
      }
    },
    "entry.html": {
      "partials": {
        "title": "{{{entry.title}}} - {{{title}}}",
        "description": "{{{entry.title}}} - {{{title}}}"
      }
    },
    "error.html": {
      "partials": {
        "title": "{{error.title}} - {{{title}}}",
        "description": "{{error.message}}"
      }
    }
  }
}