{
  "name": "akumansley",
  "locals": {
    "page_size": 100,
    "sort_by": "id",
    "background_color": "#fff",
    "sidebar_background_color": "#fafafa",
    "text_color": "rgb(34, 34, 34)",
    "link_color": "rgb(0, 75, 224)",
    "syntax_highlighter": {
      "id": "solarized-light"
    },
    "syntax_highlighter_font": {
      "id": "plex-mono",
      "font_size": 15
    },
    "body_font": {
      "id": "system-sans",
      "line_height": 1.5
    }
  },
  "enabled": true,
  "views": {
    "error.html": {
      "partials": {
        "title": "{{{error.title}}} - {{{title}}}"
      }
    },
    "entry.html": {
      "partials": {
        "title": "{{{entry.title}}} - {{{title}}}"
      }
    },
    "pagination.html": {
      "url": ["/pagination/:page"]
    }
  }
}
