    "helper/*": ["app/helper/*"],
    "config": ["config"],
    "config/*": ["config/*"],
    "documentation": ["app/documentation"],
    "documentation/*": ["app/documentation/*"],
    "scheduler/*": ["app/scheduler/*"],
    "sync": ["app/sync"],
    "sync/*": ["app/sync/*"],
    "templates": ["app/templates"],
    "templates/*": ["app/templates/*"],
    "cdn/*": ["app/cdn/*"],
    "stats/*": ["app/stats/*"]
