{
  "entry": [
    "scripts/**/*.js",
    "**/tests/**/*.js",
    "**/tests.js",
    "app/clients/icloud/macserver/index.js",
    "app/config.js",
    "app/templates/source/**/*.js",
    "app/build/plugins/*/public.js",
    "app/views/js/documentation.js",
    "app/views/js/dashboard.js",
    "app/views/dashboard/template/js/index.js",
    "app/views/dashboard/template/js/source-code-editor.js",
    "app/blog/static/fonts/build.js",
    "app/blog/static/fonts/parse.js",
    "app/documentation/featured/build.js",
    "app/documentation/featured/candidates.js",
    "config/openresty/build-config.js",
