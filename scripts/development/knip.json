  "paths": {
    "blog": ["app/blog"],
    "blog/*": ["app/blog/*"],
    "build": ["app/build"],
    "build/*": ["app/build/*"],
    "dashboard": ["app/dashboard"],
    "dashboard/*": ["app/dashboard/*"],
    "site": ["app/site"],
    "site/*": ["app/site/*"],
    "models/*": ["app/models/*"],
    "clients": ["app/clients"],
